{
    "compilerOptions": {
      "module": "es2015",
      "target": "es5",
      "rootDir": "src",
      "outDir": "dist",
      "sourceMap": true,
      "removeComments": true,
      "preserveWatchOutput": true,
      "noUnusedLocals": true,
      "noImplicitAny": false
    },
    "include": [
      "src/**/*",
    ]
  }